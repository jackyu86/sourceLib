.page {
    width: 1200px;
    margin: 30px auto 120px;
}

input[type=date],
.input-calendar {
    -webkit-appearance: none;
    background: #fff url(/static/img/icon_date.png) no-repeat right 10px center;
}

.product {
    position: relative;
    margin-bottom: 20px;
    background: #fff;
}

.product .recommend-label {
    position: absolute;
    top: 0;
    left: 0;
    background: url(../img/hot.png);
    width: 44px;
    height: 44px;
    background-size: 44px;
}

.product .header {
    padding: 30px 20px;
    display: table;
    border-bottom: 1px solid #eee;
    width: 100%;
}

.product .header .image-wrap {
    width: 100px;
    height: 42px;
    display: table-cell;
}

.product .header .header-content {
    display: table-cell;
    vertical-align: top;
    padding-left: 20px;
}

.product .header .header-content h4 {
    margin: 0 0 10px;
    line-height: 18px;
}

.product .header .header-content p {
    margin: 0;
    font-size: 12px;
    color: #999;
    line-height: 12px;
}

.product .content {
    padding: 40px 20px;
    border-bottom: 1px solid #eee;
}

.product .content .attribute-list {
    font-size: 14px;
}

.product .content .attribute-list:before, .product .content .attribute-list:after {
    content: "";
    display: block;
    clear: both;
}

.product .content .attribute {
    float: left;
    color: #999;
    line-height: 35px;
}

.product .content .attribute-name {
    margin-right: 40px;
    margin-bottom: 0;
    line-height: 46px;
    font-weight: normal;
    vertical-align: top;
}

.product .content .attribute-name:after {
    content: "";
}

.product .content .attribute-value-list {
    margin-left: 5px;
    color: #000;
    height: 35px;
    display: inline-block;
}

.product .content .attribute-value {
    display: inline-block;
    margin-right: 10px;
    height: 46px;
    line-height: 46px;
    position: relative;
}

.product .content .attribute-value .form-control {
    margin-top: 4px;
}

.product .content .attribute-value .attribute-message {
    display: none;
    position: absolute;
    left: 100%;
    top: 0;
    white-space: nowrap;
    padding-left: 15px;
}

.product .content .attribute-value .attribute-message.show {
    display: block;
}

.product .attribute .attribute-value-list .attribute-value {
    border: 1px solid #eee;
    box-sizing: border-box;
    height: 35px;
}

.product .attribute .attribute-value-list label.attribute-value {
    line-height: 20px;
}

.product .attribute .attribute-value-list .attribute-value:active,
.product .attribute .attribute-value-list .attribute-value.active {
    box-shadow: none;
    border: 2px solid #cf0014;
}

.product .attribute .attribute-value-list .attribute-value > input[type=radio] {
    display: none;
}

.product .content .liability-list {
    clear: left;
}

.product .content .liability-list:before,
.product .content .liability-list:after {
    content: "";
    display: block;
    clear: both;
}

.product .content .liability {
    display: table-row;
    padding: 10px 0;
}

.product .content .liability:before {
    content: "";
    width: 1px;
    min-height: 34px;
    margin: 10px 0;
    display: inline-block;
    vertical-align: middle;
}

.product .content .liability-name {
    display: table-cell;
    padding: 0 15px;
    max-width: 200px;
    margin: 0;
    line-height: 1;
    vertical-align: middle;
}

.product .content .liability-name.small {
    font-size: 12px;
}

.product .content .liability-name.smaller {
    font-size: 11px;
}

.product .content .liability-name.smallest {
    font-size: 10px;
    max-width: 100%;
}

.product .content .liability-value {
    display: table-cell;
    padding: 0;
    vertical-align: middle;
}

.product .content .liability-value select,
.product .content .liability-value input {
    width: auto;
    margin-right: 10px;
    display: inline-block;
}

.product .footer {
    line-height: 55px;
    padding: 20px;
    text-align: right;
}

.product .footer .guide a {
    padding: 0 10px;
}

.product .footer .sale-amount {
    font-size: 12px;
    margin-right: 40px;
}

.product .footer .sale-amount > span {
    color: #e2626f;
    margin-left: 10px;
}

.product .footer .price {
    margin-right: 30px;
}

.product .footer .original {
    font-size: 12px;
    color: #999;
    text-decoration: line-through;
    margin-right: 10px;
}

.product .footer .sale {
    color: #e2626f;
    font-size: 18px;
}

.product .footer .button-wrap {
    width: 100%;
}

.product .footer .buy {
    height: 55px;
    line-height: 55px;
    padding: 0 35px;
}

.detail-section {
    background: #fff;
}

.detail-section.fixed {
    padding-top: 70px;
}

.detail-section.fixed .nav-tabs {
    position: fixed;
    top: 0;
    left: 0;
    right: 0;
    margin: auto;
    max-width: 1200px;
    width: 100%;
    background: #fff;
    box-shadow: 0 3px 5px rgba(0, 0, 0, 0.1);
    z-index: 10;
}

.detail-section .nav-tabs > li {
    border-top: 2px solid #fff;
}

.detail-section .nav-tabs > li.active {
    border-top: 2px solid #0d9da0;
}

.detail-section .nav-tabs > li > a {
    margin-right: -1px;
    font-size: 18px;
    padding: 20px 45px;
    color: #000;
}

.detail-section .nav-tabs > li > a:hover {
    color: #0d9da0;
    background: #fff;
    border-top-color: #fff;
    border-right-color: #fff;
    border-left-color: #fff;
}

.detail-section .nav-tabs > li.active > a {
    color: #0d9da0;
    margin-right: 0;
}

.detail-section .nav-tabs > li.active > a:hover {
    color: #0d9da0;
    margin-right: 0;
    border-left-color: #eee;
    border-right-color: #eee;
}

.detail-section .banner {
    margin-bottom: 40px;
}

.detail-section .tab-content .tab-pane {
    padding: 40px;
}

.detail-section .section:first-child {
    margin-top: 0;
}

.detail-section .section {
    margin-top: 40px;
}

.detail-section .section p {
    margin: 0;
    line-height: 30px;
}

.detail-section .section .list {
    list-style-type: decimal;
}

.detail-section .sub-title {
    font-size: 18px;
    padding: 0 10px;
    color: #0d9da0;
    border-left: 4px solid #0d9da0;
    margin-bottom: 40px;
}

.detail-section .info {
    margin-bottom: 20px;
}

.detail-section .table .media {
    border: 1px solid #eee;
    margin: -1px 0 0;
}

.detail-section .table .media-left {
    text-align: center;
    vertical-align: middle;
    border-right: 1px solid #eee;
    padding: 10px;
}

.detail-section .table .media-body {
    padding: 10px;
}

.detail-section .insurance-content .type {
    width: 227px;
}

.detail-section .insurance-content .amount {
    width: 180px;
}

.detail-section .insurance-content .description {
    padding: 35px 70px;
}

.detail-section .footer {
    padding: 24px 35px;
    background: #f4f4f4;
    margin-bottom: 20px;
}

.detail-section .footer > a {
    padding: 0 18px;
}

.detail-section .guide-list:before, .detail-section .guide-list:after {
    content: " ";
    display: table;
    clear: both;
}

.detail-section .guide-list .media-left {
    padding-right: 20px;
}

.detail-section .guide-list .step {
    padding: 0 45px 40px;
    counter-increment: index;
}

.detail-section .guide-list .guide-label {
    position: absolute;
    top: 0;
    left: 0;
    width: 45px;
    height: 30px;
}

.detail-section .guide-list .index {
    width: 30px;
    height: 30px;
    text-align: center;
    line-height: 30px;
    color: #fff;
    background: #0d9da0;
}

.detail-section .guide-list .index::before {
    content: counter(index);
}

.detail-section .guide-list .arrow {
    position: absolute;
    top: 10px;
    right: 0;
    border-width: 5px 7px 5px 0;
    border-style: solid;
    border-color: transparent #f4f4f4;
}

.detail-section .guide-list .guide-content {
    margin: 0;
    width: 100%;
    background: #f4f4f4;
    padding: 40px;
}

.detail-section .guide-list .step-image {
    width: 96px;
    height: 96px;
    background: #fff;
    border: 0;
}

.detail-section .guide-list .step-name {
    margin-top: 20px;
    text-align: center;
}

.detail-section .guide-list .step-description a {
    display: block;
}

.detail-section .tips {
    margin-bottom: 50px;
}

.detail-section .tips h4 {
    margin-bottom: 30px;
}

.detail-section .tips p {
    font-size: 14px;
    line-height: 24px;
    margin: 0;
}

.detail-section .qna {
    margin-bottom: 60px;
    counter-increment: qna;
}

.detail-section .qna .question {
    margin-bottom: 20px;
}

.detail-section .qna .question::before {
    content: counter(qna) "„ÄÅ";
}

.detail-section .qna-pagination {
    text-align: center;
}

.detail-section .qna-pagination .qna-page {
    display: inline-block;
    line-height: 30px;
    font-size: 20px;
    height: 30px;
    width: 30px;
    text-align: center;
    color: #0d9da0;
    cursor: pointer;
}

.detail-section .qna-pagination .qna-page.active {
    color: #fff;
    background: #0d9da0;
}