html, body {
    border: 0;
    padding: 0;
    margin: 0;
    height: 100%;
    width: 100%;
    color: #444;
    font-family: RobotoDraft, Roboto, sans-serif;
    -webkit-font-smoothing: antialiased;
    background: #ebeef0;
}

input:-webkit-autofill,
input:-webkit-autofill:hover,
input:-webkit-autofill:focus,
input:-webkit-autofill:active {
    transition: background-color 5000s linear 0s;
    -webkit-box-shadow: 0 0 0 1000px #fff inset;
}

.top-account {
    white-space: nowrap;
}

.page {
    position: relative;
    width: 100%;
    height: 100%;
    background: #f5f5f5;
    box-sizing: border-box;
}

.dialog {
    width: 60%;
}

md-toolbar.top:not(.md-menu-toolbar) {
    background: #FFF;
    color: #333;
    box-shadow: 0 2px 5px rgba(0, 0, 0, 0.1);
}

.page .top .md-toolbar-tools .md-button, .page .top .md-toolbar-tools .md-button.md-icon-button md-icon {
    color: #333;
}

.md-toolbar-tools h1, .md-toolbar-tools h2, .md-toolbar-tools h3 {
    font-weight: bold;
    color: #333;
}

.page md-toolbar.md-default-theme:not(.md-menu-toolbar) md-icon, .page md-toolbar:not(.md-menu-toolbar) md-icon {
    color: #333;
}

.directory-tree {
    width: 300px;
}

a {
    outline: none !important;
}

ul, li {
    list-style: none;
    padding: 0;
    margin-top: 0;
}

.search-box {
    margin: 0;
}

.searching .fa-search:before {
    content: "\f00d" !important;
}

.search-box input::-webkit-input-placeholder {
    color: rgba(0, 0, 0, 0.26);
}

.search-box input:-moz-placeholder {
    color: rgba(0, 0, 0, 0.26);
}

.search-box md-input-container {
    margin: 7px 0;
}

.search-box input {
    border: 0;
    color: rgba(0, 0, 0, 0.87);
    background: transparent;
    width: 100%;
}

.search-box input:focus {
    outline: none;
}

.search-box .md-errors-spacer {
