    transform: rotate(45deg);
    position: absolute;
    left: 4px;
    top: -5px;
    display: table;
    width: 6.67px;
    height: 13.33px;
    border-width: 2px;
    border-style: solid;
    border-top: 0;
    border-left: 0;
    content: '';
}

